@page
@model Otel_ProjeOdev.Pages.Politika.DetayModel
@{
}

<center style="margin-top:150px">
@foreach (var item in Model.politikaListe)
{
        <b style="font-size:36px">Politika Kod: <x style="color:blue"> @item.PolitikaKod </x> </b>
    <br />
        <b style="font-size:36px">Politika Kayıt Zamanı: <x style="color:blue"> @item.PolitikaKayitZamani </x> </b>
    <br />
        <b style="font-size:36px">Politika Ayrılma Zamanı: <x style="color:blue"> @item.PolitikaAyrilmaZamani </x> </b>
    <br />
        <b style="font-size:36px">Politika İptal Süresi: <x style="color:blue"> @item.PolitikaIptalSuresi </x> </b>
   <br />
}
</center>


